<!-- list.ejs -->
<html>

<head>
    <%- include('partials/head.ejs') %>
</head>

<body class="container">
    <%- include('partials/topnav.ejs') %>
    <h1>
        List
    </h1>
    <table class='table'>
        <thead>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </thead>
        <tbody>
            <% users.forEach(function(user) { %>
                <tr>
                    <td><a href="/users/<%= user.id %>">
                            <%= user.id %>
                        </a></td>
                    <td>
                        <%= user.firstName %>
                            <%= user.lastName %>
                    </td>
                    <td>
                        <%= user.email %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
        </div>
</body>

</html>